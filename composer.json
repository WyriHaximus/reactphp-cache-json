{
    "name": "wyrihaximus/react-cache-json",
    "description": "JSON encode/decode decorator for react/cache",
    "license": "MIT",
    "authors": [
        {
            "name": "Cees-Jan Kiewiet",
            "email": "ceesjank@gmail.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "daverandom/exceptional-json": "^1.0.4",
        "react/cache": "^1.2.0",
        "react/promise": "^2.10.0"
    },
    "require-dev": {
        "wyrihaximus/async-test-utilities": "^7.2"
    },
    "autoload": {
        "psr-4": {
            "WyriHaximus\\React\\Cache\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "WyriHaximus\\Tests\\React\\Cache\\": "tests/"
        }
    },
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "infection/extension-installer": true,
            "ergebnis/composer-normalize": true,
            "icanhazstring/composer-unused": true
        },
        "platform": {
            "php": "8.2.13"
        },
        "sort-packages": true
    },
    "scripts": {
        "post-install-cmd": [
            "composer normalize"
        ],
        "post-update-cmd": [
            "composer normalize"
        ]
    }
}
