{
    "name": "wyrihaximus/react-cache-json",
    "description": "JSON encode/decode decorator for react/cache",
    "license": "MIT",
    "authors": [
        {
            "name": "Cees-Jan Kiewiet",
            "email": "ceesjank@gmail.com"
        }
    ],
    "require": {
        "php": "^8 || ^7.4",
        "daverandom/exceptional-json": "^1.0.4",
        "react/cache": "^1.2.0",
        "react/promise": "^2.10.0"
    },
    "require-dev": {
        "wyrihaximus/async-test-utilities": "^3.4.20 || ^4.0.0 || ^7.0.0"
    },
    "config": {
        "platform": {
            "php": "7.4.7"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "WyriHaximus\\React\\Cache\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "WyriHaximus\\Tests\\React\\Cache\\": "tests/"
        }
    },
    "scripts": {
        "post-install-cmd": [
            "composer normalize"
        ],
        "post-update-cmd": [
            "composer normalize"
        ]
    }
}
